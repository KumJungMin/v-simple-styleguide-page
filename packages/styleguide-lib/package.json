{
  "name": "vue-styleguide-lib",
  "version": "1.0.0",
  "description": "Vue 3 interactive styleguide library",
  "keywords": ["vue", "vue3", "styleguide", "component", "playground", "documentation"],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/KumJungMin/v-simple-styleguide-page.git",
    "directory": "packages/styleguide-lib"
  },
  "homepage": "https://github.com/KumJungMin/v-simple-styleguide-page#readme",
  "bugs": {
    "url": "https://github.com/KumJungMin/v-simple-styleguide-page/issues"
  },
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./types": {
      "import": "./dist/types.esm.js",
      "require": "./dist/types.js",
      "types": "./dist/types.d.ts"
    },
    "./core": {
      "import": "./dist/core.esm.js",
      "require": "./dist/core.js",
      "types": "./dist/core.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm run clean && pnpm run build",
    "publish:patch": "npm version patch && npm publish",
    "publish:minor": "npm version minor && npm publish",
    "publish:major": "npm version major && npm publish"
  },
  "dependencies": {
    "markdown-it": "^13.0.0",
    "highlight.js": "^11.9.0"
  },
  "devDependencies": {
    "@types/markdown-it": "^14.1.2",
    "@vitejs/plugin-vue": "^4.0.0",
    "typescript": "^5.0.0",
    "vite": "^4.0.0",
    "vite-plugin-dts": "^3.0.0"
  },
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
} 